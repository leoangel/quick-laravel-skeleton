{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-ide-helper": "v2.4.3",
        "fideloper/proxy": "~3.3",
        "gregwar/captcha": "v1.1.6",
        "guzzlehttp/guzzle": "6.3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mews/captcha": "2.2.0",
        "mike42/escpos-php": "v2.0.1",
        "milon/barcode": "5.3.6",
        "overtrue/laravel-pinyin": "3.0.5",
        "pda/pheanstalk": "v3.1.0",
        "php-curl-class/php-curl-class": "8.1.0",
        "phpoffice/phpspreadsheet": "1.3.0",
        "predis/predis": "v1.1.1",
        "riverslei/payment": "v4.1.7",
        "yansongda/pay": "v2.3.2"
    },
    "repositories":[
        {
            "type":"path",
            "url":"./vendor/lib/barcode-bakery/barcode-common"
        },
        {
            "type":"path",
            "url":"./vendor/lib/barcode-bakery/barcode-1d"
        }
    ],
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
